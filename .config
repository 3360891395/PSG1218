# 编译K2-PSG1218固件:
CONFIG_TARGET_ramips=y
CONFIG_TARGET_ramips_mt7620=y
CONFIG_TARGET_ramips_mt7620_DEVICE_phicomm_psg1218a=y

# IPv6支持:
# CONFIG_PACKAGE_dnsmasq_full_dhcpv6 is not set
# CONFIG_PACKAGE_ip6tables is not set
# CONFIG_PACKAGE_ipv6helper is not set

# 常用LuCI插件选择:
CONFIG_DEFAULT_luci-app-filetransfer=y
CONFIG_PACKAGE_luci-app-ddns=y
CONFIG_DEFAULT_ddns-scripts_aliyun=y
CONFIG_DEFAULT_ddns-scripts_dnspod=y
CONFIG_PACKAGE_luci-app-arpbind=y
CONFIG_PACKAGE_luci-app-autoreboot=y
CONFIG_PACKAGE_luci-app-guest-wifi=y
CONFIG_PACKAGE_luci-app-ramfree=y
# CONFIG_PACKAGE_luci-app-ssr-plus is not set
# CONFIG_PACKAGE_luci-app-ssr-plus_INCLUDE_V2ray_plugin is not set
# CONFIG_PACKAGE_luci-app-ssr-plus_INCLUDE_V2ray is not set
CONFIG_PACKAGE_luci-app-upnp=y
CONFIG_PACKAGE_luci-app-wol=y
CONFIG_PACKAGE_luci-app-vlmcsd=y
CONFIG_PACKAGE_luci-app-ttyd=y
# CONFIG_PACKAGE_luci-app-vsftpd is not set
# CONFIG_PACKAGE_luci-app-zerotier is not set
# CONFIG_PACKAGE_luci-app-flowoffload is not set
# CONFIG_PACKAGE_luci-app-unblockmusic is not set
# CONFIG_PACKAGE_luci-app-accesscontrol is not set
# CONFIG_PACKAGE_luci-app-adbyby-plus is not set

#添加clash支持
CONFIG_PACKAGE_luci-app-clash=y

# LuCI主题:
# CONFIG_PACKAGE_luci-theme-opentomcat is not set
CONFIG_PACKAGE_luci-theme-netgear=y
CONFIG_DEFAULT_luci-theme-netgear=y
# CONFIG_DEFAULT_luci-theme-bootstrap is not set
# CONFIG_PACKAGE_luci-theme-argon is not set
# CONFIG_DEFAULT_luci-theme-darkmatter is not set
# CONFIG_PACKAGE_luci-theme-material is not set
